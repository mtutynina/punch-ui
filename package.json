{
  "name": "punch-ui",
  "version": "1.0.5",
  "description": "UI Components for KnockoutJS",
  "main": "dist/punch-ui",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "webpack": "webpack --mode=development",
    "webpack-prod": "webpack --mode=production",
    "build": "npm run webpack",
    "lint": "eslint --ext ts src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtutynina/punch-ui.git"
  },
  "author": "Mariya Tutynina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mtutynina/punch-ui/issues"
  },
  "homepage": "https://github.com/mtutynina/punch-ui#readme",
  "files": [
    "dist/punch-ui.js",
    "dist/index.d.ts",
    "README.md"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1",
      "^tests/(.*)": "<rootDir>/tests/$1"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "awesome-typescript-loader": "^5.2.1",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.24.0",
    "eslint-config-typescript-strict": "^1.0.1",
    "jest": "^26.6.3",
    "node": "^15.14.0",
    "raw-loader": "^4.0.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "knockout": "^3.5.1"
  }
}
