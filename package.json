{
  "name": "punch-ui",
  "version": "1.0.0",
  "description": "UI Components for KnockoutJS",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtutynina/punch-ui.git"
  },
  "author": "Mariya Tutynina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mtutynina/punch-ui/issues"
  },
  "homepage": "https://github.com/mtutynina/punch-ui#readme",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1",
      "^tests/(.*)": "<rootDir>/tests/$1"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "jest": "^26.6.3",
    "node": "^15.14.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "knockout": "^3.5.1"
  }
}
